#include <bits/stdc++.h>

int findDuplicate(vector<int> &arr, int n){
	// Write your code here.
	    vector<int> p(n,0);
            for(int i=0;i<n;i++){
                if(p[arr[i]]==0){
                    p[arr[i]]+=1;
                }
                else
                    return arr[i];  
            }
        return 0;
}
